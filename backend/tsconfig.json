{
    "compilerOptions": {
      "lib": ["ESNext"],
      "target": "ESNext",
      "module": "commonjs",
      "outDir": "../dist/backend",
      "rootDir": "./",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["./**/*"],
    "exclude": ["node_modules"]
  }